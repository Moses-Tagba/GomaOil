<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="l1ig.dev.isig.gomaoil">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/gomaoil_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.D1">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCCV4dhJQIAhipl2JfG90Ljpb7qbFOVG_o" />


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.D1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".gui.sendMessageActivity"
            android:label="@string/info" />

        <activity android:name=".gui.AbonneActivity"
            android:label="Abonnement" />

        <activity android:name=".gui.StationActivity"
            android:label="Recherche"
            android:theme="@style/AppTheme" />

        <activity android:name=".gui.MapsActivity"
            android:label="Maps"
            android:theme="@style/AppTheme" />

    </application>

</manifest>
